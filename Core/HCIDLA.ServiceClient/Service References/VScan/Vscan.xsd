    <xs:sequence>
      <xs:element minOccurs="0" name="DefinitionDate" type="xs:dateTime" />
      <xs:element minOccurs="0" name="DefinitionRevisionNumber" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="FileName" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="FileSize" type="xs:double" />
