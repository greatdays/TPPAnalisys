@section Scripts {
    <!-- Load Dashboard.js first -->
    <script src="~/pagescripts/dashboard/dashboard.js"></script>

    <!-- Then safely call its methods -->
    <script>
        $(document).ready(function () {
            Dashboard.init();
            Dashboard.PopulateMyProjectData();



            $(".tab").click(function() {
                var tabId = $(this).data("tab");

                // Remove active from all tabs and contents
                $(".tab, .tab-content").removeClass("active");

                // Activate clicked tab and its content
                $(this).addClass("active");
                $("#tab-" + tabId).addClass("active");
            });

        });


        function oncreateAPNChange()
        {
            var selectedValue = $('input[name="ISCreateAPN"]:checked').val();
            if (selectedValue === "True") {
                $("#divCreateAPN").show();
            } else {$("#divCreateAPN").hide();
            }
        
        }

        function toggleManualAddress()
        {
            var selectedValue = $('input[name="IsAddAddress"]:checked').val();
            if (selectedValue === "true") {
                $("#manualAddressFields").show();
            } else {$("#manualAddressFields").hide();
            }

        }

